{
  "name": "stvbyr/php-design-patterns",
  "description": "Short and concrete examples of common design patterns",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Stvbyr\\PhpDesignPatterns\\": "src/",
      "Stvbyr\\PhpDesignPatterns\\Test\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "stvbyr"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "allow-plugins": {
      "phpro/grumphp": true
    }
  },
  "require-dev": {
    "phpmd/phpmd": "^2.11",
    "phpro/grumphp": "^1.8",
    "phpunit/phpunit": "^9.5",
    "psalm/plugin-phpunit": "^0.16.1",
    "friendsofphp/php-cs-fixer": "^3.4",
    "vimeo/psalm": "^4.20"
  }
}
